generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(cuid())
  slug      String   @unique
  name      String
  price     String?
  region    String?
  sku       String?
  image     String?
  images    String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
